{
  "name": "moleculer-io-3",
  "version": "0.0.1",
  "description": "An API GateWay service for moleculerjs using socket.io v3 - Originally by @tiaod",
  "main": "lib/index.js",
  "directories": {
    "example": "examples",
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "dev": "babel src -d lib --watch",
    "example": "npm install && node examples/simple/index.js",
    "test": "ava",
    "compile": "babel -d lib/ src/",
    "prepare": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bsovs/moleculer-io.git"
  },
  "keywords": [
    "socket.io",
    "moleculer",
    "microservice",
    "API Gateway"
  ],
  "author": "tiaod/bsovs",
  "license": "MIT",
  "dependencies": {
    "chalk": "^2.4.1",
    "lodash": "^4.17.11",
    "socket.io": "^3.1.1"
  },
  "devDependencies": {
    "ava": "^1.2.1",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "express": "^4.16.3",
    "fs-extra": "^7.0.1",
    "moleculer": "^0.13.5",
    "moleculer-web": "^0.8.5",
    "socket.io-client": "^3.1.1",
    "socket.io-redis": "^6.0.1",
    "winston": "^3.2.1"
  },
  "bugs": {
    "url": "https://github.com/bsovs/moleculer-io/issues"
  },
  "homepage": "https://github.com/bsovs/moleculer-io#readme"
